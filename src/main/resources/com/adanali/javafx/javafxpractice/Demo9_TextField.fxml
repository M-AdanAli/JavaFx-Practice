   <children>
      <Label fx:id="myLabel" layoutX="162.0" layoutY="37.0" text="Welcome">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="36.0" layoutY="140.0" text="Enter your age : ">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="myTextField" layoutX="36.0" layoutY="189.0" prefHeight="36.0" prefWidth="320.0" />
      <Button fx:id="myButton" layoutX="176.0" layoutY="276.0" mnemonicParsing="false" onAction="#submit" text="Submit">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
